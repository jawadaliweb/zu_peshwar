<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:sweet="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@+id/colorImg">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_Bus_Route_04A"/>
            <corners android:radius="20dp"/>
            <size android:height="@dimen/_100sdp" android:width="16dp"/>
        </shape>
    </item>
    <item android:gravity="top|center_vertical|center_horizontal|center" android:top="@dimen/_5sdp">
        <shape android:shape="oval">
            <solid android:color="@color/colorWhite"/>
            <size android:height="12dp" android:width="12dp"/>
        </shape>
    </item>
    <item android:gravity="bottom|center_vertical|center_horizontal|center" android:bottom="@dimen/_5sdp">
        <shape android:shape="oval">
            <solid android:color="@color/colorWhite"/>
            <size android:height="12dp" android:width="12dp"/>
        </shape>
    </item>
</layer-list>
